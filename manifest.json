{
    "name": "Page Saver",
    "description": "You can save various pages to look later",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }, 
    "permissions": ["bookmarks", "unlimitedStorage", "tabs"]
}